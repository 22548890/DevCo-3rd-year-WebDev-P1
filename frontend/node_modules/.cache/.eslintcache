 = ({\n    onChange,\n    value\n}) =>\n    <div className=\"field\">\n        <label htmlFor=\"email\">\n            Developer Email / Company Name:\n        </label>\n        <input\n            id=\"email\"\n            type=\"text\"\n            onChange={onChange}\n            maxlength=\"25\"\n            value={value}\n            placeholder=\"jbloggs@mail.com\"\n            required />\n    </div>\n\nconst Password = ({\n    onChange,\n    value\n}) =>\n    <div className=\"field\">\n        <label htmlFor=\"password\">\n            Password:\n        </label>\n        <input\n            id=\"password\"\n            type=\"password\"\n            onChange={onChange}\n            maxlength=\"25\"\n            value={value}\n            placeholder=\"password\"\n            required />\n    </div>\n\nconst handleDevReg = () => {\n    window.location.pathname = \"/DevReg\";\n};\n\nconst handleComReg = () => {\n    window.location.pathname = \"/ComReg\";\n};\n\nconst Edit = ({\n    onSubmit,\n    children,\n}) =>\n    <div className=\"card\">\n        <form onSubmit={onSubmit}>\n            {children}\n            <button type=\"submit\" className=\"login\">LOGIN </button>\n            <div>\n                <button className=\"DRegBtn\" onClick={handleDevReg}>Developer Registration</button>\n                <button className=\"CRegBtn\" onClick={handleComReg}>Company Registration</button>\n            </div>\n\n        </form>\n    </div>\n\nconst Profile = ({\n    onSubmit,\n    email,\n    password,\n}) =>\n    <div className=\"card\">\n        <form onSubmit={onSubmit}>\n            <h1>Successfully Logged in</h1>\n            <div className=\"email\">{email}</div>\n            <div className=\"password\">{password}</div>\n\n        </form>\n    </div>\n\n\nclass Login extends React.Component {\n    state = {\n        email: '',\n        password: '',\n        active: 'edit',\n    }\n\n    editEmail = e => {\n        const email = e.target.value;\n        this.setState({\n            email,\n        });\n    }\n\n    editPassword = e => {\n        const password = e.target.value;\n        this.setState({\n            password,\n        });\n    }\n\n    handleSubmit = e => {\n        e.preventDefault();\n\n\n        let data = this.state;\n        const requestOpt = {\n            method: 'POST',\n            headers: { 'Content-Type': 'application/json' },\n            body: JSON.stringify({\n                'email': data.email,\n                'password': data.password,\n            }),\n        }\n        async function fetchFunc() {\n            return await fetch('http://127.0.0.1:5000/login', requestOpt)\n                .then(response => response.json())\n                .catch(error => console.log(error));\n        }\n        (async () => {\n            let info = await fetchFunc();\n            if (info.success) { // correct login info\n                // let activeP = data.active === 'edit' ? 'profile' : 'edit';\n                // this.setState({\n                //     active: activeP,\n                // })\n                <Redirect to=\"/\" />\n                localStorage.setItem(\"isAuthenticated\", \"true\");\n                window.location.pathname = \"/\";\n\n            } else {\n                alert(info.msg);\n                \n            }\n        })()\n    }\n\n    render() {\n        const {\n            email,\n            password,\n            active } = this.state;\n\n        return (\n            <div>\n\n                {(active === 'edit') ? (\n\n                    <Edit onSubmit={this.handleSubmit}>\n                        <ImgUpload src={logo} className=\"loginimg\" />\n                        <Email onChange={this.editEmail} value={email} />\n                        <Password onChange={this.editPassword} value={password} />\n                    </Edit>\n\n\n                ) : (\n                    <Profile\n                        onSubmit={this.handleSubmit}\n                        email={email}\n                        password={password}\n                    />)}\n\n            </div>\n        )\n    }\n}\n// ReactDOM.render(\n//     <>\n//         <Login />\n//     </>\n//     ,\n//     document.getElementById('root')\n// )\n\nexport default Login;","/home/chris/data/cs334/group_32/frontend/src/Components/Home.js",["50"],[],"import React, { PureComponent } from 'react'\n\nfunction Home()\n{\n    const handleLogout = () => {\n        localStorage.clear();\n        window.location.pathname = \"/login\";\n    };\n    return (\n        <nav>\n            <h1>Home</h1>\n            <button onClick={handleLogout} >Logout </button>\n        </nav>\n    )\n}\n\nexport default Home;","/home/chris/data/cs334/group_32/frontend/src/Components/Com_Registration.js",["51"],[],"/home/chris/data/cs334/group_32/frontend/src/Components/Dev_Registration.js",["52"],[],"/home/chris/data/cs334/group_32/frontend/src/Components/ProtectedRoute.js",[],[],{"ruleId":"53","severity":1,"message":"54","line":1,"column":17,"nodeType":"55","messageId":"56","endLine":1,"endColumn":26},{"ruleId":"53","severity":1,"message":"57","line":1,"column":17,"nodeType":"55","messageId":"56","endLine":1,"endColumn":25},{"ruleId":"53","severity":1,"message":"58","line":3,"column":8,"nodeType":"55","messageId":"56","endLine":3,"endColumn":16},{"ruleId":"53","severity":1,"message":"59","line":6,"column":8,"nodeType":"55","messageId":"56","endLine":6,"endColumn":14},{"ruleId":"53","severity":1,"message":"60","line":7,"column":8,"nodeType":"55","messageId":"56","endLine":7,"endColumn":14},{"ruleId":"53","severity":1,"message":"61","line":1,"column":17,"nodeType":"55","messageId":"56","endLine":1,"endColumn":30},{"ruleId":"53","severity":1,"message":"58","line":3,"column":8,"nodeType":"55","messageId":"56","endLine":3,"endColumn":16},{"ruleId":"53","severity":1,"message":"58","line":3,"column":8,"nodeType":"55","messageId":"56","endLine":3,"endColumn":16},"no-unused-vars","'Compon